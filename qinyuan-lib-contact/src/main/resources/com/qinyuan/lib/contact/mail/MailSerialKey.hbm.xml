<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.qinyuan.lib.contact.mail">
    <class name="MailSerialKey" table="mail_serial_key">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="userId" column="user_id"/>
        <property name="serialKey" column="serial_key"/>
        <property name="sendTime" column="send_time"/>
        <property name="responseTime" column="response_time"/>
        <property name="mailType" column="mail_type"/>
    </class>
</hibernate-mapping>
